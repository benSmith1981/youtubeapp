  pod "iOS-GTLYouTube"

# Pods for YoutubeApplication

  target 'YoutubeApplicationTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'YoutubeApplicationUITests' do
    inherit! :search_paths
    # Pods for testing
  end
end
